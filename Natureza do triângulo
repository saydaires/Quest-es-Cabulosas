programa{
	funcao inicio(){
		real a, b, c

		escreva("Medida do lado a = ")
		leia(a)

		limpa()
		
		escreva("Medida do lado b = ")
		 leia(b)

		limpa()
		
		escreva("Medida do lado c = ")
		leia(c)

		limpa()

		se((a == b) e (b == c)){
			//(a==b) e (b==c) implica verdadeiramente que (a==c).
			escreva("Triângulo EQUILÁTERO")
		
		
		}senao se((a == b) ou (a == c) ou (b == c)){
			escreva("Triângulo ISÓSCELES")
			
		
		}senao{
			//Se o triângulo não for equilátero ou isósceles, será escaleno.
			escreva("Triângulo ESCALENO")
		}
				
	}
}

